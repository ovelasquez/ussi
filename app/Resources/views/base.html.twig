                            <div class="x_panel">
                                <div class="x_title">
                                    <h2>{% block Subtitulo %} USSI UBV {% endblock %} </h2>
                                    <ul class="nav navbar-right panel_toolbox submenu">
                                        {% block paneltoolbox %} 
                                            <li><a class="collapse-link" ><i class="fa fa-chevron-up"></i></a>
                                            </li>
