                                            </li>
                                        {% endblock %}          
                                    </ul>
                                    <div class="clearfix"></div>
                                </div>
                                <!--div class="x_content"-->
                                    {% block body %} Contenido de mi vista va aquí  :)  {% endblock %}                                    
                                <!--/div-->
                            </div>
                        </div>
