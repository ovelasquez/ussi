{% extends 'base.html.twig' %}

{% block body %}
    <h1>Signosvitalessuministrado</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ signosVitalesSuministrado.id }}</td>
            </tr>
            <tr>
                <th>Dato</th>
                <td>{{ signosVitalesSuministrado.nombre }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ signosVitalesSuministrado.valor }}</td>
            </tr>            
            <tr>
                <th>Fecha</th>
                <td>{% if signosVitalesSuministrado.consulta.fecha %}{{ signosVitalesSuministrado.consulta.fecha|date('d-m-Y H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('signosvitales_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('signosvitales_edit', { 'id': signosVitalesSuministrado.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <button type="submit" class="btn btn-danger"><i class="fa fa-trash"></i> Eliminar </button>
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
